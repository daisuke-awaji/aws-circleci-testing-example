{
  "name": "aws-circleci-testing-exapmle",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "codecov",
    "start": "ts-node src/main.ts",
    "build:lambda": "NODE_ENV=production tsc",
    "build:lambda-layer": "mkdir -p layer_modules && cp package.json layer_modules && npm i --production --prefix layer_modules && ts-node scripts/node-modules-archives.ts",
    "deploy:lambda": "sh scripts/deploy_lambda.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.3",
    "archiver": "^3.1.1",
    "codecov": "^3.5.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "aws-sdk": "^2.521.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverage": true
  }
}
